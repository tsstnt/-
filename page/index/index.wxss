#root {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #8ed145;
  height: 100%
}
.avatar {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin: 100rpx 0;
}
.userName{
  font-size: 40rpx;
  font-weight: bolder;
  margin: 100rpx 0;
}
.goStudy {
  width: 500rpx;
  height: 100rpx;
  border: 1px solid #eee;
  line-height: 100rpx;
  text-align: center;
  border-radius: 10rpx;
}