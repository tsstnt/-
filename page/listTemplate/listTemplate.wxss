#tmpContainer {
  display: flex;
  flex-direction: column;
}
.avatar_date {
  padding: 10rpx;
}

.avatar_date image {
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}

.avatar_date text {
  font-size: 24rpx;
  vertical-align: middle;
}

.company {
  font-size: 36rpx;
  font-weight: bold;
  padding-left: 10rpx;
}
.tmpImg {
  width: 100%;
  height: 460rpx;
}

.content {
  font-size: 28rpx;
  text-indent: 28rpx;
}

.star_view image {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.star_view text {
 font-size: 20rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}